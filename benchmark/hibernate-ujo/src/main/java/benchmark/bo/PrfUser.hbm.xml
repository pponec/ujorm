<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="org.ujorm.hibernateSupport.UjoPropertyAccessor" package="benchmark.bo">
  <class name="PrfUser" table="prf_user">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property length="8" name="personalId"/>
    <property length="128" name="forename"/>
    <property length="128" name="surname"/>
  </class>
</hibernate-mapping>
