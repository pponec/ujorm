<?xml version="1.0" encoding="UTF-8"?>
<body>
    <database id="Database">
        <schema>db1</schema>
        <dialect>org.ujoframework.orm.dialect.H2Dialect</dialect>
        <table id="ord_order">
            <name>ord_order</name>
            <alias>ord_order</alias>
            <column id="id">
                <name>id</name>
                <primaryKey>true</primaryKey>
                <dbType>BIGINT</dbType>
                <mandatory>true</mandatory>
            </column>
            <column id="usrId">
                <name>usrId</name>
                <dbType>INT</dbType>
            </column>
            <column id="description">
                <name>DESCR</name>
                <dbType>VARCHAR</dbType>
                <mandatory>true</mandatory>
                <maxLength>128</maxLength>
            </column>
            <column id="date">
                <name>date</name>
                <dbType>TIMESTAMP</dbType>
            </column>
            <relation2m id="items">
                <name>items</name>
            </relation2m>
        </table>
        <table id="ord_item">
            <name>ord_item</name>
            <alias>ord_item</alias>
            <column id="id">
                <name>id</name>
                <primaryKey>true</primaryKey>
                <dbType>BIGINT</dbType>
                <mandatory>true</mandatory>
            </column>
            <column id="usrId">
                <name>usrId</name>
                <dbType>INT</dbType>
            </column>
            <column id="description">
                <name>description</name>
                <dbType>VARCHAR</dbType>
                <maxLength>128</maxLength>
            </column>
            <column id="fk_order">
                <name>fk_order</name>
                <dbType>INT</dbType>
            </column>
        </table>
        <table id="view_order" view="true">
            <name>ord_order</name>
            <alias>ord_order</alias>
            <column id="id">
                <name>id</name>
                <primaryKey>true</primaryKey>
                <dbType>BIGINT</dbType>
                <mandatory>true</mandatory>
            </column>
            <column id="itemCount">
                <name>itemCount</name>
                <dbType>INT</dbType>
            </column>
            <select>SELECT ord_order.id, count(*) AS itemCount FROM db1.ord_order ord_order, db1.ord_item ord_item WHERE ord_order.id = ord_item.fk_order GROUP BY ord_order.id ORDER BY ord_order.id</select>
        </table>
        <table id="db1" view="true">
            <name>ord_order</name>
            <alias>ord_order</alias>
            <relation2m id="ord_order">
                <name>ord_order</name>
            </relation2m>
            <relation2m id="ord_item">
                <name>ord_item</name>
            </relation2m>
            <relation2m id="view_order">
                <name>view_order</name>
            </relation2m>
        </table>
        <jdbcUrl>jdbc:h2:mem:db1</jdbcUrl>
        <jdbcDriver>org.h2.Driver</jdbcDriver>
        <user>sa</user>
    </database>
    <parameters></parameters>
</body>
