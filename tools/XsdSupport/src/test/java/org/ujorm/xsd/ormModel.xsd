<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:simpleType name="CheckReport">
        <xs:restriction base="xs:string">
            <xs:enumeration value="SKIP"></xs:enumeration>
            <xs:enumeration value="QUOTE_SQL_NAMES"></xs:enumeration>
            <xs:enumeration value="WARNING"></xs:enumeration>
            <xs:enumeration value="EXCEPTION"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CommentPolicy">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ON_ANY_CHANGE"></xs:enumeration>
            <xs:enumeration value="FOR_NEW_OBJECT"></xs:enumeration>
            <xs:enumeration value="NEVER"></xs:enumeration>
            <xs:enumeration value="ALWAYS"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="LazyLoading">
        <xs:restriction base="xs:string">
            <xs:enumeration value="DISABLED"></xs:enumeration>
            <xs:enumeration value="ALLOWED_USING_OPEN_SESSION"></xs:enumeration>
            <xs:enumeration value="ALLOWED_ANYWHERE_WITH_WARNING"></xs:enumeration>
            <xs:enumeration value="ALLOWED_ANYWHERE_WITH_STACKTRACE"></xs:enumeration>
            <xs:enumeration value="ALLOWED_ANYWHERE"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CachePolicy">
        <xs:restriction base="xs:string">
            <xs:enumeration value="PROTECTED_CACHE"></xs:enumeration>
            <xs:enumeration value="SOLID_CACHE"></xs:enumeration>
            <xs:enumeration value="NO_CACHE"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DbType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Automatic"></xs:enumeration>
            <xs:enumeration value="INT"></xs:enumeration>
            <xs:enumeration value="BOOLEAN"></xs:enumeration>
            <xs:enumeration value="TINYINT"></xs:enumeration>
            <xs:enumeration value="SMALLINT"></xs:enumeration>
            <xs:enumeration value="BIGINT"></xs:enumeration>
            <xs:enumeration value="DECIMAL"></xs:enumeration>
            <xs:enumeration value="FLOAT"></xs:enumeration>
            <xs:enumeration value="DOUBLE"></xs:enumeration>
            <xs:enumeration value="REAL"></xs:enumeration>
            <xs:enumeration value="TIME"></xs:enumeration>
            <xs:enumeration value="DATE"></xs:enumeration>
            <xs:enumeration value="TIMESTAMP"></xs:enumeration>
            <xs:enumeration value="BINARY"></xs:enumeration>
            <xs:enumeration value="VARCHAR"></xs:enumeration>
            <xs:enumeration value="VARCHAR_IGNORECASE"></xs:enumeration>
            <xs:enumeration value="CHAR"></xs:enumeration>
            <xs:enumeration value="BLOB"></xs:enumeration>
            <xs:enumeration value="CLOB"></xs:enumeration>
            <xs:enumeration value="NULL"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Orm2ddlPolicy">
        <xs:restriction base="xs:string">
            <xs:enumeration value="DO_NOTHING"></xs:enumeration>
            <xs:enumeration value="WARNING"></xs:enumeration>
            <xs:enumeration value="CREATE_DDL"></xs:enumeration>
            <xs:enumeration value="CREATE_OR_UPDATE_DDL"></xs:enumeration>
            <xs:enumeration value="VALIDATE"></xs:enumeration>
            <xs:enumeration value="INHERITED"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="MoreParams">
        <xs:sequence>
            <xs:element name="DialectMySqlEngineType" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="printAllJoinedTables" type="xs:boolean" minOccurs="0"></xs:element>
            <xs:element name="enableToUnlockImmutableMeta-model" type="xs:boolean" minOccurs="0"></xs:element>
            <xs:element name="extentedIndexNameStrategy" type="xs:boolean" minOccurs="0"></xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="MetaParams">
        <xs:sequence>
            <xs:element name="cachePolicy" type="CachePolicy" minOccurs="0"></xs:element>
            <xs:element name="tableAliasPrefix" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="tableAliasSuffix" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="sequenceCache" type="xs:int" minOccurs="0"></xs:element>
            <xs:element name="lazyLoading" type="LazyLoading" minOccurs="0"></xs:element>
            <xs:element name="orm2ddlPolicy" type="Orm2ddlPolicy" minOccurs="0"></xs:element>
            <xs:element name="commentPolicy" type="CommentPolicy" minOccurs="0"></xs:element>
            <xs:element name="saveConfigToFile" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="initializationBatch" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="typeService" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="metaDbService" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="sqlNameProvider" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="checkKeywords" type="CheckReport" minOccurs="0"></xs:element>
            <xs:element name="maxItemCountForIN" type="xs:int" minOccurs="0"></xs:element>
            <xs:element name="sequenceSchemaSymbol" type="xs:boolean" minOccurs="0"></xs:element>
            <xs:element name="inheritanceMode" type="xs:boolean" minOccurs="0"></xs:element>
            <xs:element name="insertMultirowItemLimit" type="xs:int" minOccurs="0"></xs:element>
            <xs:element name="moreParams" type="MoreParams" minOccurs="0"></xs:element>
            <xs:element name="logMetamodelInfo" type="xs:boolean" minOccurs="0"></xs:element>
            <xs:element name="logSqlMultiInsert" type="xs:boolean" minOccurs="0"></xs:element>
            <xs:element name="logbackLoggingSupport" type="xs:boolean" minOccurs="0"></xs:element>
            <xs:element name="logValueLengthLimit" type="xs:int" minOccurs="0"></xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="MetaProcedure">
        <xs:sequence>
            <xs:element name="name" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="schema" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="parameter" type="MetaColumn" minOccurs="0" maxOccurs="unbounded"></xs:element>
        </xs:sequence>
        <xs:attribute name="id" type="xs:string"></xs:attribute>
    </xs:complexType>
    <xs:complexType name="MetaRelation2Many">
        <xs:sequence>
            <xs:element name="name" type="xs:string" minOccurs="0"></xs:element>
        </xs:sequence>
        <xs:attribute name="id" type="xs:string"></xs:attribute>
    </xs:complexType>
    <xs:complexType name="MetaColumn">
        <xs:sequence>
            <xs:element name="name" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="primaryKey" type="xs:boolean" minOccurs="0"></xs:element>
            <xs:element name="dbType" type="DbType" minOccurs="0"></xs:element>
            <xs:element name="mandatory" type="xs:boolean" minOccurs="0"></xs:element>
            <xs:element name="maxLength" type="xs:int" minOccurs="0"></xs:element>
            <xs:element name="precision" type="xs:int" minOccurs="0"></xs:element>
            <xs:element name="default" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="index" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="uniqueIndex" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="constraintName" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="converter" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="comment" type="xs:string" minOccurs="0"></xs:element>
        </xs:sequence>
        <xs:attribute name="id" type="xs:string"></xs:attribute>
    </xs:complexType>
    <xs:complexType name="MetaTable">
        <xs:sequence>
            <xs:element name="name" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="alias" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="schema" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="readOnly" type="xs:boolean" minOccurs="0"></xs:element>
            <xs:element name="orm2ddlPolicy" type="Orm2ddlPolicy" minOccurs="0"></xs:element>
            <xs:element name="sequence" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="select" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="comment" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="column" type="MetaColumn" minOccurs="0" maxOccurs="unbounded"></xs:element>
            <xs:element name="relation2m" type="MetaRelation2Many" minOccurs="0" maxOccurs="unbounded"></xs:element>
        </xs:sequence>
        <xs:attribute name="id" type="xs:string"></xs:attribute>
        <xs:attribute name="view" type="xs:boolean"></xs:attribute>
    </xs:complexType>
    <xs:complexType name="MetaDatabase">
        <xs:sequence>
            <xs:element name="schema" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="readOnly" type="xs:boolean" minOccurs="0"></xs:element>
            <xs:element name="dialect" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="jdbcUrl" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="jdbcDriver" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="user" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="password" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="jndi" type="xs:string" minOccurs="0" maxOccurs="unbounded"></xs:element>
            <xs:element name="sequencer" type="xs:string" minOccurs="0"></xs:element>
            <xs:element name="orm2ddlPolicy" type="Orm2ddlPolicy" minOccurs="0"></xs:element>
            <xs:element name="table" type="MetaTable" minOccurs="0" maxOccurs="unbounded"></xs:element>
            <xs:element name="procedure" type="MetaProcedure" minOccurs="0" maxOccurs="unbounded"></xs:element>
        </xs:sequence>
        <xs:attribute name="id" type="xs:string"></xs:attribute>
    </xs:complexType>
    <xs:complexType name="MetaRoot">
        <xs:sequence>
            <xs:element name="database" type="MetaDatabase" minOccurs="0" maxOccurs="unbounded"></xs:element>
            <xs:element name="parameters" type="MetaParams" minOccurs="0"></xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="body" type="MetaRoot"></xs:element>
</xs:schema>